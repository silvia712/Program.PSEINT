Algoritmo ejer11
	
	definir num,factorial,i Como Entero;
	
	num=0;
	factorial=1;
	i=1;
	
	escribir "escribe un numero ";
	leer num;
	
	Para i=1 Hasta num Con Paso 1 Hacer
		factorial=factorial*i;
